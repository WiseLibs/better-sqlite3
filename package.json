{
  "name": "better-sqlite3",
  "version": "5.4.4",
  "description": "The fastest and simplest library for SQLite3 in Node.js.",
  "homepage": "http://github.com/danielbarela/better-sqlite3",
  "author": "Joshua Wise <joshuathomaswise@gmail.com>",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/danielbarela/better-sqlite3.git"
  },
  "dependencies": {
    "bindings": "1.5.0",
    "integer": "danielbarela/integer#v2.1.1",
    "tar": "^4.4.10",
    "prebuild-install": "^5.3.2",
    "prebuild": "^9.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cli-color": "^1.4.0",
    "fs-extra": "^8.1.0",
    "mocha": "^6.2.0",
    "nodemark": "^0.3.0",
    "sqlite": "^3.0.3"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "download": "sh ./deps/download.sh",
    "lzz": "lzz -hx hpp -sx cpp -k BETTER_SQLITE3 -d -hl -sl -e ./src/better_sqlite3.lzz",
    "prepublishOnly": "npm run lzz",
    "install-debug": "node-gyp rebuild --debug",
    "rebuild": "npm run lzz && node-gyp rebuild",
    "rebuild-debug": "npm run lzz && node-gyp rebuild --debug",
    "test": "mocha --exit --slow=75 --timeout=5000",
    "benchmark": "node benchmark",
    "prebuild-electron": "prebuild -r electron -t 2.0.0 -t 3.0.0 -t 4.0.0 -t 5.0.0 -t 6.0.0 -t 7.0.0 --include-regex 'better_sqlite3.node$'",
    "prebuild": "prebuild -r node -t 8.0.0 -t 10.0.0 -t 12.0.0 --include-regex 'better_sqlite3.node$'"
  },
  "license": "MIT",
  "keywords": [
    "sql",
    "sqlite",
    "sqlite3",
    "transactions",
    "user-defined functions",
    "aggregate functions",
    "window functions",
    "database"
  ]
}
